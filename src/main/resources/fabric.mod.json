{
  "schemaVersion": 1,
  "id": "heartvisualizer",
  "version": "${version}",
  "name": "Heart Visualizer",
  "description": "Displays health in hearts, above head, or numerically",
  "authors": ["SweetyBanana", "Fcostar"],
  "contact": {
    "homepage": "https://modrinth.com/mod/heartvisualizer",
    "sources": "https://github.com/SweetyBananaK/HeartVisualizer",
    "issues": "https://github.com/SweetyBananaK/HeartVisualizer/issues"
  },
  "license": "MIT",
  "icon": "assets/heartvisualizer/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "github.sweety_banana.heartvisualizer.client.HeartVisualizerClient"
    ],
    "main": [
      "github.sweety_banana.heartvisualizer.HeartVisualizer"
    ],
    "modmenu": [
      "github.sweety_banana.heartvisualizer.client.config.HeartVisualizerModMenu"
    ]
  },
  "mixins": [
    "heartvisualizer.mixins.json",
    {
      "config": "heartvisualizer.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config2": ">=14.0.0"
  },
  "suggests": {
    "modmenu": "*"
  }
}
